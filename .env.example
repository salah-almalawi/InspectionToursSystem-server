# Example environment configuration for InspectionToursSystem

# MongoDB connection
MONGODB_URI=mongodb://localhost:27017/ITS

# Application port
PORT=3000

# JWT settings
JWT_SECRET=your_jwt_secret
JWT_EXPIRES_IN=1h

# Logger level
LOG_LEVEL=info

# Comma-separated allowed origins for CORS. Leave empty to allow all
CORS_ALLOWED_ORIGINS=http://localhost:3000

# Rate limiting (defaults: 15 minutes window, 100 requests)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100